#NewsCarousel {
	list-style-type: none;
	margin: 0;
  	padding: 10px;
  	background-color: #f9f9f9; 
}
#NewsCarousel li {
	margin-bottom: 10px;
}
#NewsCarousel li:hover{
	background-color: #fdfdfd;
}
#NewsCarousel li.campaign b{
	color: red; 
}
#NewsCarousel li.new-product b{
	color: blue; 
}
#NewsCarousel li.news b{
	color: green; 
}